<Application
    x:Class="Kok.Test.WpfDemo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Kok.Test.WpfDemo"
    xmlns:views="clr-namespace:Kok.Test.WpfDemo.Views"
    xmlns:vm="clr-namespace:Kok.Test.WpfDemo.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Controls/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <DataTemplate DataType="{x:Type vm:DialogDemoViewModel}">
                <views:DialogDemoView />
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:LogMonitorViewModel}">
                <views:LogMonitorView />
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>